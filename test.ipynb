    "function time_evolution(ψ::Vector{ComplexF64}, L)\n",
    "\n",
    "    U_odd  = odd_layer(L)\n",
    "    U_even = even_layer(L)\n",
    "\n",
    "    # One full brick-wall step:\n",
    "    U_step = U_even * U_odd\n",
    "    ψ_new = U_step * ψ\n",
    "\n",
    "    return normalize!(ψ_new)\n",
    "end"
   ]
