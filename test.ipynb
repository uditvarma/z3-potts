    "function Hamiltonian(L::Int)\n",
    "    id, Z, X = get_potts_operators()\n",
    "    dim = 3^L\n",
    "    \n",
    "    # Initialize Hamiltonian; J = 1\n",
    "    H = spzeros(ComplexF64, dim, dim)\n",
    "    \n",
    "    for i in 1:L\n",
    "        # Periodic boundary condition\n",
    "        ip = mod1(i + 1, L)\n",
    "\n",
    "        J = rand()\n",
    "        g = rand()\n",
    "        \n",
    "        ops_H_1 = fill(id, L)\n",
    "        ops_H_1[i] = X'\n",
    "        ops_H_1[ip] = X\n",
    "        H += J * build_term(ops_H_1)\n",
    "        \n",
    "       \n",
    "        ops_H_2 = fill(id, L)\n",
    "        ops_H_2[i] = Z\n",
    "        H += g * build_term(ops_H_2)\n",
    "    end\n",
    "    \n",
    "    H += adjoint(H)\n",
    "\n",
    "    return H\n",
    "end"
   ]
