    "function random_product_state(L::Int)\n",
    "    ψ = nothing\n",
    "    for i in 1:L\n",
    "        θ1, θ2 = rand() * π, rand() * π\n",
    "        ϕ1, ϕ2 = rand() * 2π, rand() * 2π\n",
    "        c1 = cos(θ1 / 2)\n",
    "        c2 = exp(im * ϕ1) * sin(θ1 / 2) * sin(θ2 / 2)\n",
    "        c3 = exp(im * ϕ2) * sin(θ1 / 2) * cos(θ2 / 2)\n",
    "        site_state = [c1, c2, c3]\n",
    "        ψ = i == 1 ? site_state : kron(ψ, site_state)\n",
    "    end\n",
    "    return ψ / norm(ψ)\n",
    "end"
   ]
